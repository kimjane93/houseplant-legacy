<%- include('../partials/header') %> 


<% users.forEach((u)=>{ %>

    <div class="card" style="width: 18rem;">
        <img src="<%= u.avatar %>" class="card-img-top" alt="plant-image">
        <div class="card-body">
            <h5 class="card-title"><%= u.name %></h5>
        </div>
        <div class="card-body">
            <a href="/users/<%= u._id %>" class="card-link">Peek Their Story (and Garden)!</a>
        </div>
    </div>
<% }) %> 


<%- include('../partials/footer') %> 